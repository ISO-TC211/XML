  xmlns:gml="http://www.opengis.net/gml/3.2"
  xmlns:xlink="http://www.w3.org/1999/xlink">
  <mrl:statement>
    <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:statement/gco:CharacterString</gco:CharacterString>
  </mrl:statement>
  <mrl:scope>
    <mcc:MD_Scope>
      <mcc:level>
        <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" codeListValue="anyValidURI">/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:scope/gmd:DQ_Scope/gmd:level/gmd:MD_ScopeCode</mcc:MD_ScopeCode>
      </mcc:level>
      <mcc:extent gco:nilReason="withheld"/>
        <!-- can't have an extent unless mrl and gex are both imported into a containing schema so that 
          the abstract extent class in mcc has concrete implementation -->
      <mcc:levelDescription>
        <mcc:MD_ScopeDescription>
          <mcc:other>
            <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:scope/gmd:DQ_Scope/gmd:levelDescription/gmd:MD_ScopeDescription/gmd:other/gco:CharacterString</gco:CharacterString>
          </mcc:other>
        </mcc:MD_ScopeDescription>
      </mcc:levelDescription>
    </mcc:MD_Scope>
  </mrl:scope>
  <mrl:source>
    <mrl:LI_Source id="lineageSource">
      <mrl:description>
        <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:source/gmd:LI_Source/gmd:description/gco:CharacterString</gco:CharacterString>
      </mrl:description>
      <mrl:sourceSpatialResolution>
        <mri:MD_Resolution>
          <mri:equivalentScale>
            <mri:MD_RepresentativeFraction>
              <mri:denominator>
                <gco:Integer>999999</gco:Integer>
              </mri:denominator>
            </mri:MD_RepresentativeFraction>
          </mri:equivalentScale>
        </mri:MD_Resolution>
      </mrl:sourceSpatialResolution>
      <mrl:sourceReferenceSystem gco:nilReason="withheld" xlink:href="http:some.referenceSystem.org/myreferenceSystem"/>
      <!-- to implement a source reference system, mrl and mrs must both be imported into a container scheme to provide 
           a concrete implementation of the abstract class-->
       <mrl:sourceCitation>
        <cit:CI_Citation>
          <cit:title>
            <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:source/gmd:LI_Source/gmd:sourceCitation/gmd:CI_Citation/gmd:title/gco:CharacterString</gco:CharacterString>
          </cit:title>
          <cit:date gco:nilReason="missing"/>
        </cit:CI_Citation>
      </mrl:sourceCitation>
      <mrl:scope>
        <mcc:MD_Scope>
          <mcc:level gco:nilReason="missing"/>
          <mcc:extent xlink:href="external link to extent" gco:nilReason="withheld"/>
          <!-- can't have an extent unless mrl and gex are both imported into a containing schema so that 
          the abstract extent class in mcc has concrete implementation -->
           <mcc:levelDescription gco:nilReason="missing"/>
        </mcc:MD_Scope>
      </mrl:scope>
      <mrl:sourceStep xlink:href="#lineageProcessStep"/>
      <mrl:sourceStep>
        <mrl:LI_ProcessStep id="embeddedLineageProcessStep">
          <mrl:description>
            <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:processStep/gmd:LI_ProcessStep/gmd:description/gco:CharacterString</gco:CharacterString>
          </mrl:description>
          <mrl:stepDateTime>
            <gml:TimeInstant gml:id="d1e2288">
              <gml:timePosition>2005-05-05T05:05:05</gml:timePosition>
            </gml:TimeInstant>
          </mrl:stepDateTime>
          <mrl:processor>
            <cit:CI_Responsibility>
              <cit:role>
                <cit:CI_RoleCode codeList="http://codelistsR.us/myFavCodelist" codeListValue="processor">processor</cit:CI_RoleCode>
              </cit:role>
              <cit:party>
                <cit:CI_Individual>
                  <cit:name>
                    <gco:CharacterString>Jose Theprocessor</gco:CharacterString>
                  </cit:name>
                </cit:CI_Individual>
              </cit:party>
            </cit:CI_Responsibility>
          </mrl:processor>
        </mrl:LI_ProcessStep>
      </mrl:sourceStep>
    </mrl:LI_Source>
  </mrl:source>
  <mrl:processStep>
    <mrl:LI_ProcessStep id="lineageProcessStep">
      <mrl:description>
        <gco:CharacterString>/*/gmd:dataQualityInfo.3/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:processStep/gmd:LI_ProcessStep/gmd:description/gco:CharacterString</gco:CharacterString>
      </mrl:description>
      <mrl:stepDateTime>
        <gml:TimeInstant gml:id="d1e2221">
          <gml:timePosition>2005-05-05T05:05:05</gml:timePosition>
        </gml:TimeInstant>
      </mrl:stepDateTime>
      <mrl:processor>
        <cit:CI_Responsibility>
          <cit:role>
            <cit:CI_RoleCode codeList="codeListLocation#CI_RoleCode" codeListValue="anyValidURI"/>
          </cit:role>
          <cit:party>
            <cit:CI_Individual>
              <cit:name>
                <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:processStep/gmd:LI_ProcessStep/gmd:processor/gmd:CI_ResponsibleParty/gmd:individualName/gco:CharacterString</gco:CharacterString>
              </cit:name>
            </cit:CI_Individual>
          </cit:party>
        </cit:CI_Responsibility>
      </mrl:processor>
      <mrl:source xlink:href="#lineageSource"/>
      <mrl:source>
        <mrl:LI_Source id="embeddedLineageSource">
          <mrl:description>
            <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:source/gmd:LI_Source/gmd:description/gco:CharacterString</gco:CharacterString>
          </mrl:description>
          <mrl:sourceSpatialResolution>
            <mri:MD_Resolution>
              <mri:equivalentScale>
                <mri:MD_RepresentativeFraction>
                  <mri:denominator>
                    <gco:Integer>999999</gco:Integer>
                  </mri:denominator>
                </mri:MD_RepresentativeFraction>
              </mri:equivalentScale>
            </mri:MD_Resolution>
          </mrl:sourceSpatialResolution>
          <mrl:sourceCitation>
            <cit:CI_Citation>
              <cit:title>
                <gco:CharacterString>/*/gmd:dataQualityInfo/gmd:DQ_DataQuality/gmd:lineage/gmd:LI_Lineage/gmd:source/gmd:LI_Source/gmd:sourceCitation/gmd:CI_Citation/gmd:title/gco:CharacterString</gco:CharacterString>
              </cit:title>
              <cit:date gco:nilReason="missing"/>
            </cit:CI_Citation>
          </mrl:sourceCitation>
          <mrl:scope>
            <mcc:MD_Scope>
              <mcc:level gco:nilReason="missing"/>
              <mcc:extent gco:nilReason="withheld" xlink:href="link to external extent object"/>
                <!-- can't have an extent unless mrl and gex are both imported into a containing schema so that 
          the abstract extent class in mcc has concrete implementation -->
               <mcc:levelDescription gco:nilReason="missing"/>
            </mcc:MD_Scope>
          </mrl:scope>
        </mrl:LI_Source>
      </mrl:source>
    </mrl:LI_ProcessStep>
  </mrl:processStep>
</mrl:LI_Lineage>

