   <!--============================= Codelists =======================================-->
   <!-- MD_ClassificationCode  -->
   <cat:codelistItem>
      <cat:CT_Codelist id="MD_ClassificationCode">
         <cat:identifier>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_ClassificationCode</gco:ScopedName>
         </cat:identifier>
         <cat:name>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_ClassificationCode</gco:ScopedName>
         </cat:name>
         <cat:definition>
            <gco:CharacterString>name of the handling restrictions on the
               resource</gco:CharacterString>
         </cat:definition>
         <cat:description>
            <gco:CharacterString>name of the handling restrictions on the
               resource</gco:CharacterString>
         </cat:description>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_unclassified">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_unclassified</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>available for general disclosure</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_restricted">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_restricted</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>not for general disclosure</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_confidential">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_confidential</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>available for someone who can be entrusted with
                     information</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_secret">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_secret</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>kept or meant to be kept private, unknown, or hidden from all
                     but a select group of people</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_topSecret">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_topSecret</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>of the highest secrecy</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_SBU">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_SBU</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>although unclassified, requires strict controls over its
                     distribution</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_forOfficialUseOnly">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_forOfficialUseOnly</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>unclassified information that is to be used only for official
                     purposes determined by the designating body</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_protected">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_protected</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compromise of the information could cause
                     damage</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ClassificationCode_limitedDistribution">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ClassificationCode_limitedDistribution</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>desimination limited by designating
                     body</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
      </cat:CT_Codelist>
   </cat:codelistItem>
   <!-- MD_RestrictionCode  -->
   <cat:codelistItem>
      <cat:CT_Codelist id="MD_RestrictionCode">
         <cat:identifier>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_RestrictionCode</gco:ScopedName>
         </cat:identifier>
         <cat:name>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_RestrictionCode</gco:ScopedName>
         </cat:name>
         <cat:definition>
            <gco:CharacterString>limitation(s) placed upon the access or use of the
               data</gco:CharacterString>
         </cat:definition>
         <cat:description>
            <gco:CharacterString>limitation(s) placed upon the access or use of the
               data</gco:CharacterString>
         </cat:description>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_copyright">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_copyright</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>exclusive right to the publication, production, or sale of
                     the rights to a literary, dramatic, musical, or artistic work, or to the use of
                     a commercial print or label, granted by law for a specified period of time to
                     an author, composer, artist, distributor</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_patent">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_patent</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>government has granted exclusive right to make, sell, use or
                     license an invention or discovery</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_patentPending">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_patentPending</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>produced or sold information awaiting a
                     patent</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_trademark">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_trademark</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>a name, symbol, or other device identifying a product,
                     officially registered and legally restricted to the use of the owner or
                     manufacturer</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_licence">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_licence</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>formal permission to do something</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_intellectualPropertyRights">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_intellectualPropertyRights</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>rights to financial benefit from and control of distribution
                     of non-tangible property that is a result of creativity</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_restricted">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_restricted</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>withheld from general circulation or
                     disclosure</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_otherRestrictions">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_otherRestrictions</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>limitation not listed</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_unrestricted">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_unrestricted</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>no constraints exist</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_licenceUnrestricted">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_licenceUnrestricted</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>formal permission not required to use the
                     resource</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_licenceEndUser">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_licenceEndUser</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>formal permission required for a person or an entity to use
                     the resource and that may differ from the person that orders or purchases
                     it</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_licenceDistributor">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_licenceDistributor</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>formal permission required for a person or an entity to
                     commercialize or distribute the resource</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_private">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_private</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>protects rights of individual or organisations from
                     observation, intrusion, or attention of others </gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_statutory">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_statutory</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>prescribed by law</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_confidential">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_confidential</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>not available to the public contains information that could
                     be prejudicial to a commercial, industrial, or national
                     interest</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_SBU">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_SBU</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>although unclassified, requires strict controls over its
                     distribution. </gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_RestrictionCode_in-confidence">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_RestrictionCode_in-confidence</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>with trust</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
      </cat:CT_Codelist>
   </cat:codelistItem>
