   <!--============================= Codelists =======================================-->
   <!-- MD_ReferenceSystemTypeCode  -->
   <cat:codelistItem>
      <cat:CT_Codelist id="MD_ReferenceSystemTypeCode">
         <cat:identifier>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_ReferenceSystemTypeCode</gco:ScopedName>
         </cat:identifier>
         <cat:name>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_ReferenceSystemTypeCode</gco:ScopedName>
         </cat:name>
         <cat:definition>
            <gco:CharacterString>defines type of reference system used</gco:CharacterString>
         </cat:definition>
         <cat:description>
            <gco:CharacterString>defines type of reference system used</gco:CharacterString>
         </cat:description>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundEngineeringParametric">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundEngineeringParametric</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound statio-parametric coordinate reference system
                     containing an engineering coordinate reference system and a parametric
                     reference system. e.g. [local] x, y, pressure</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue
               id="MD_ReferenceSystemTypeCode_compoundEngineeringParametricTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundEngineeringParametricTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound statio-parametric-temporal coordinate reference
                     system containing an engineering, a parametric, and a temporal coordinate
                     reference system. e.g. [local] x, y, pressure, time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundEngineeringTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundEngineeringTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing an engineering and a temporal coordinate reference system. e.g.
                     [local] x, y, time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundEngineeringVertical">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundEngineeringVertical</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatial reference system containing a horizontal
                     engineering coordinate reference system and a vertical coordinate reference
                     system. e.g. [local] x, y, height</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue
               id="MD_ReferenceSystemTypeCode_compoundEngineeringVerticalTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundEngineeringVerticalTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing an engineering, a vertical, and a temporal coordinate reference
                     system. e.g. [local] x, y, height, time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundGeodeticParametric">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundGeodeticParametric</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound statio-parametric coordinate reference system
                     containing a 2 dimensional geographic horizontal coordinate reference system
                     and a parametric reference system. e.g. latitude, longitude,
                     pressure</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundGeodeticParametricTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundGeodeticParametricTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound statio-parametric-temporal coordinate reference
                     system containing a 2 dimensional geographic horizontal, a parametric and a
                     temporal coordinate reference system. e.g. latitude, longitude, pressure,
                     time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundGeographic2DTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundGeographic2DTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing a 2 dimensional geographic horizontal coordinate reference system
                     and a temporal reference system. e.g. latitude, longitude,
                     time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundGeographic2DVertical">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundGeographic2DVertical</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound coordinate reference system in which one constituent
                     coordinate reference system is a horizontal geodetic coordinate reference
                     system and one is a vertical coordinate reference system. e.g. latitude,
                     longitude, [gravity-related] height or depth</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundGeographicVerticalTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundGeographicVerticalTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing a 2 dimensional geographic horizontal, a vertical, and a temporal
                     coordinate reference system. e.g. latitude, longitude, height,
                     time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundGeographic3DTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundGeographic3DTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing a 3 dimensional geographic and a temporal coordinate reference
                     system. e.g. latitude, longitude, ellipsoidal height,
                     time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundProjected2DParametric">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundProjected2DParametric</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound statio-parametric coordinate reference system
                     containing a projected horizontal coordinate reference system and a parametric
                     reference system. e.g. easting, northing, density</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue
               id="MD_ReferenceSystemTypeCode_compoundProjected2DParametricTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundProjected2DParametricTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound statio-parametric-temporal coordinate reference
                     system containing a projected horizontal, a parametric, and a temporal
                     coordinate reference system. e.g. easting, northing, density,
                     time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundProjectedTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundProjectedTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing a projected horizontal and a temporal coordinate reference system.
                     e.g. easting, northing, time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundProjectedVertical">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundProjectedVertical</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatial reference system containing a horizontal
                     projected coordinate reference system and a vertical coordinate reference
                     system. e.g. easting, northing, [gravity-related] height or
                     depth</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_compoundProjectedVerticalTemporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_compoundProjectedVerticalTemporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>compound spatio-temporal coordinate reference system
                     containing a projected horizontal, a vertical, and a temporal coordinate
                     reference system. e.g. easting, northing, height, time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_engineering">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_engineering</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>coordinate reference system based on an engineering datum
                     (datum describing the relationship of a coordinate system to a local
                     reference). e.g. [local] x,y</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_engineeringDesign">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_engineeringDesign</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>engineering coordinate reference system in which the base
                     representation of a moving object is specified. e.g. [local]
                     x,y</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_engineeringImage">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_engineeringImage</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>coordinate reference system based on an image datum
                     (engineering datum which defines the relationship of a coordinate system to an
                     image). e.g. row, column</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_geodeticGeocentric">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_geodeticGeocentric</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>geodetic CRS having a Cartesian 3D coordinate system. e.g.
                     [geocentric] X,Y,Z</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_geodeticGeographic2D">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_geodeticGeographic2D</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>geodetic CRS having an ellipsoidal 2D coordinate system. e.g.
                     latitude, longitude</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_geodeticGeographic3D">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_geodeticGeographic3D</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>geodetic CRS having an ellipsoidal 3D coordinate system. e.g.
                     latitude, longitude, ellipsoidal height</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_geographicIdentifier">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_geographicIdentifier</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>spatial reference in the form of a label or code that
                     identifies a location. e.g. post code</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_linear">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_linear</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>set of Linear Referencing Methods and the policies, records
                     and procedures for implementing them. reference system that identifies a
                     location by reference to a segment of a linear geographic feature and distance
                     along that segment from a given point. e.g. x km along
                     road</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_parametric">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_parametric</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>coordinate reference system based on a parametric datum
                     (datum describing the relationship of a parametric coordinate system to an
                     object). e.g. pressure</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_projected">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_projected</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>coordinate reference system derived from a two-dimensional
                     geodetic coordinate reference system by applying a map projection. e.g.
                     easting, northing</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_temporal">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_temporal</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>reference system against which time is measured. e.g.
                     time</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ReferenceSystemTypeCode_vertical">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ReferenceSystemTypeCode_vertical</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>one-dimensional coordinate reference system based on a
                     vertical datum (datum describing the relation of gravity-related heights or
                     depths to the Earth). e.g. [gravity-related] height or
                     depth</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
      </cat:CT_Codelist>
   </cat:codelistItem>
