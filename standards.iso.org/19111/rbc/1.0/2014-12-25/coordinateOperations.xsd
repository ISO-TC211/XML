  <annotation>
    <documentation>The Coordinate Operations package describes the information required to transform or convert coordinates form one coordinate reference system to another.</documentation>
  </annotation>
  <include schemaLocation="coordinateReferenceSystems.xsd"/>
  <include schemaLocation="rbc.xsd"/>
  <!--<import namespace="http://standards.iso.org/19139/gco/1.0/2014-12-25" schemaLocation="http://standards.iso.org/ittf/PubliclyAvailableStandards/ISO_19139_Schemas/gco/gco.xsd"/>-->
  <import namespace="http://standards.iso.org/19139/gco/1.0/2014-12-25" schemaLocation="../../../../19139/gco/1.0/2014-12-25/gco.xsd"/>
