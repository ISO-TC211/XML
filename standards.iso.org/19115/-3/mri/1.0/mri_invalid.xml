<?xml version="1.0" encoding="UTF-8"?>
<mdb:MD_Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://standards.iso.org/19115/-3/mdt/1.0 ../../mdt/1.0/mdt.xsd" xmlns:mdb="http://standards.iso.org/19115/-3/mdb/1.0" xmlns:gco="http://standards.iso.org/19115/-3/gco/1.0"
 xmlns:mcc="http://standards.iso.org/19115/-3/mcc/1.0" xmlns:mri="http://standards.iso.org/19115/-3/mri/1.0" xmlns:lan="http://standards.iso.org/19115/-3/lan/1.0"
 xmlns:srv="http://standards.iso.org/19115/-3/srv/2.0" xmlns:cit="http://standards.iso.org/19115/-3/cit/1.0">
   <mdb:metadataIdentifier></mdb:metadataIdentifier>
   
   <!-- Some rules applies to dataset only -->
   <mdb:metadataScope>
      <mdb:MD_MetadataScope>
         <mdb:resourceScope>
            <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" 
               codeListValue="dataset">dataset</mcc:MD_ScopeCode>
         </mdb:resourceScope>
      </mdb:MD_MetadataScope>
   </mdb:metadataScope>
   
   
   <mdb:contact></mdb:contact>
   <mdb:dateInfo></mdb:dateInfo>
   <mdb:identificationInfo>
      <mri:MD_DataIdentification>
         <mri:citation></mri:citation>
         <mri:abstract></mri:abstract>         
         
         <!-- Invalid for rule rule.mri.topicategoryfordsandseries
         <mri:topicCategory>
            <mri:MD_TopicCategoryCode>boundaries</mri:MD_TopicCategoryCode>
         </mri:topicCategory>
          -->
         
         <!-- Invalid for rule rule.mri.associatedresource -->
         <mri:associatedResource>
            <mri:MD_AssociatedResource>
               <mri:associationType></mri:associationType>
               <mri:metadataReference uuidref=""/>
            </mri:MD_AssociatedResource>
         </mri:associatedResource>
         <mri:associatedResource>
            <mri:MD_AssociatedResource>
               <mri:associationType></mri:associationType>
            </mri:MD_AssociatedResource>
         </mri:associatedResource>
         <mri:associatedResource>
            <mri:MD_AssociatedResource>
               <mri:name>
                  <cit:CI_Citation>
                     <cit:title gco:nilReason="missing">
                        <gco:CharacterString></gco:CharacterString>
                     </cit:title>
                  </cit:CI_Citation>
               </mri:name>
               <mri:associationType></mri:associationType>
            </mri:MD_AssociatedResource>
         </mri:associatedResource>

         <mri:defaultLocale>
            <lan:PT_Locale>
               <lan:language></lan:language>
               <lan:characterEncoding>
                  <lan:MD_CharacterSetCode codeList="" codeListValue=""/>
               </lan:characterEncoding>
            </lan:PT_Locale>
         </mri:defaultLocale>
         
      </mri:MD_DataIdentification>
   </mdb:identificationInfo>
   <mdb:identificationInfo>
      <srv:SV_ServiceIdentification>
         <mri:citation></mri:citation>
         <mri:abstract></mri:abstract>
         <mri:associatedResource>
            <mri:MD_AssociatedResource>
               <mri:name>
                  <cit:CI_Citation>
                     <cit:title></cit:title>
                  </cit:CI_Citation>
               </mri:name>
               <mri:associationType></mri:associationType>
            </mri:MD_AssociatedResource>
         </mri:associatedResource>
         <srv:serviceType></srv:serviceType>
      </srv:SV_ServiceIdentification>
   </mdb:identificationInfo>
</mdb:MD_Metadata>
