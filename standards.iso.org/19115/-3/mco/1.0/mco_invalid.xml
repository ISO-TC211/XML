<?xml version="1.0" encoding="UTF-8"?>
<mdb:MD_Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://standards.iso.org/iso/19115/-3/mdb/1.0 ../../mds/1.0/mds.xsd" 
 xmlns:mco="http://standards.iso.org/iso/19115/-3/mco/1.0" 
 xmlns:mdb="http://standards.iso.org/iso/19115/-3/mdb/1.0"
 xmlns:mcc="http://standards.iso.org/iso/19115/-3/mcc/1.0" 
 xmlns:gco="http://standards.iso.org/iso/19115/-3/gco/1.0" 
 xmlns:mri="http://standards.iso.org/iso/19115/-3/mri/1.0"
 xmlns:cit="http://standards.iso.org/iso/19115/-3/cit/1.0">
   <mdb:metadataIdentifier></mdb:metadataIdentifier>
   
   <!-- Some rules applies to dataset only -->
   <mdb:metadataScope>
      <mdb:MD_MetadataScope>
         <mdb:resourceScope>
            <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" 
               codeListValue="dataset">dataset</mcc:MD_ScopeCode>
         </mdb:resourceScope>
      </mdb:MD_MetadataScope>
   </mdb:metadataScope>
   
   <mdb:contact></mdb:contact>
   <mdb:dateInfo></mdb:dateInfo>
   <mdb:identificationInfo>
      <mri:MD_DataIdentification>
         <mri:citation></mri:citation>
         <mri:abstract></mri:abstract>
         <mri:resourceConstraints>
            <mco:MD_Constraints>
               <mco:releasability>
                  <!-- Invalid for rule.mco-releasability -->
                  <mco:MD_Releasability/>                 
               </mco:releasability>
            </mco:MD_Constraints>
         </mri:resourceConstraints>
         
         <!-- Invalid for rule.mco-legalconstraintdetails -->
         <mri:resourceConstraints>
            <mco:MD_LegalConstraints>
            </mco:MD_LegalConstraints>
         </mri:resourceConstraints>
         <mri:resourceConstraints>
            <mco:MD_LegalConstraints>
               <mco:accessConstraints>
                  <mco:MD_RestrictionCode codeList="" codeListValue=""/>
               </mco:accessConstraints>
            </mco:MD_LegalConstraints>
         </mri:resourceConstraints>
         <mri:resourceConstraints>
            <mco:MD_LegalConstraints>
               <mco:otherConstraints>
                  <gco:CharacterString></gco:CharacterString>
               </mco:otherConstraints>
            </mco:MD_LegalConstraints>
         </mri:resourceConstraints>
         
         <!-- Invalid for rule.mco-legalconstraint-other -->
         <mri:resourceConstraints>
            <mco:MD_LegalConstraints>
               <mco:accessConstraints>
                  <mco:MD_RestrictionCode codeList="" codeListValue="topSecret"/>
               </mco:accessConstraints>
               <mco:accessConstraints>
                  <mco:MD_RestrictionCode codeList="" codeListValue="otherRestrictions"/>
               </mco:accessConstraints>
               
               <mco:useConstraints>
                  <mco:MD_RestrictionCode codeList="" codeListValue="otherRestrictions"/>
               </mco:useConstraints>
               
               <mco:otherConstraints>
                  <gco:CharacterString></gco:CharacterString>
               </mco:otherConstraints>
               
            </mco:MD_LegalConstraints>
         </mri:resourceConstraints>
         
      </mri:MD_DataIdentification>
   </mdb:identificationInfo>
</mdb:MD_Metadata>
