         </lan:language>
         <lan:characterEncoding>
            <lan:MD_CharacterSetCode codeList="standards.iso.org/19115/-3/lan/1.0/codelists.xml#MD_CharacterSetCode" codeListValue="UTF-8">UTF-8</lan:MD_CharacterSetCode>
         </lan:characterEncoding>
      </lan:PT_Locale>
   </cat:locale>
   <!--============================= Codelists =======================================-->
   <!-- MD_CoverageContentTypeCode  -->
   <cat:codelistItem>
      <cat:CT_Codelist id="MD_CoverageContentTypeCode">
         <cat:identifier>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_CoverageContentTypeCode</gco:ScopedName>
         </cat:identifier>
         <cat:name>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_CoverageContentTypeCode</gco:ScopedName>
         </cat:name>
         <cat:definition>
            <gco:CharacterString>specific type of information represented in the
               cell</gco:CharacterString>
         </cat:definition>
         <cat:description>
            <gco:CharacterString>specific type of information represented in the
               cell</gco:CharacterString>
         </cat:description>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_image">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_image</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>meaningful numerical representation of a physical parameter
                     that is not the actual value of the physical parameter</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_thematicClassification">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_thematicClassification</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>code value with no quantitative meaning, used to represent a
                     physical quantity</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_physicalMeasurement">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_physicalMeasurement</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>value in physical units of the quantity being
                     measured</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_auxillaryInformation">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_auxillaryInformation</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>data, usually a physical measurement, used to support the
                     calculation of the primary physicalMeasurement coverages in the dataset (e.g.
                     grid of aerosol optical thickness used in the calculation of a sea surface
                     temperature product).</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_qualityInformation">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_qualityInformation</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>data used to characterize the quality of the
                     physicalMeasurement coverages in the dataset. Typically included in a
                     gmi:QE_CoverageResult.</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_referenceInformation">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_referenceInformation</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>reference information used to support the calculation or use
                     of the physicalMeasurement coverages in the dataset (e.g. grids of
                     latitude/longitude used to geolocate the physical
                     measurements).</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_modelResult">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_modelResult</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>resources with values that are calculated using a model
                     rather than being observed or calculated from
                     observations.</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_CoverageContentTypeCode_coordinate">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_CoverageContentTypeCode_coordinate</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>data used to provide coordinate axis values
                  </gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
      </cat:CT_Codelist>
   </cat:codelistItem>
   <!-- MD_ImagingConditionCode  -->
   <cat:codelistItem>
      <cat:CT_Codelist id="MD_ImagingConditionCode">
         <cat:identifier>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_ImagingConditionCode</gco:ScopedName>
         </cat:identifier>
         <cat:name>
            <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
               >MD_ImagingConditionCode</gco:ScopedName>
         </cat:name>
         <cat:definition>
            <gco:CharacterString>code which indicates conditions which may affect the
               image</gco:CharacterString>
         </cat:definition>
         <cat:description>
            <gco:CharacterString>code which indicates conditions which may affect the
               image</gco:CharacterString>
         </cat:description>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_blurredImage">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_blurredImage</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>portion of the image is blurred</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_cloud">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_cloud</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>portion of the image is partially obscured by cloud
                     cover</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_degradingObliquity">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_degradingObliquity</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>acute angle between the plane of the ecliptic (the plane of
                     the Earth s orbit) and the plane of the celestial equator</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_fog">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_fog</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>portion of the image is partially obscured by
                     fog</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_heavySmokeOrDust">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_heavySmokeOrDust</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>portion of the image is partially obscured by heavy smoke or
                     dust</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_night">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_night</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>image was taken at night</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_rain">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_rain</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>image was taken during rainfall</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_semiDarkness">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_semiDarkness</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>image was taken during semi-dark conditions; twilight
                     conditions</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_shadow">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_shadow</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>portion of the image is obscured by
                     shadow</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_snow">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_snow</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>portion of the image is obscured by
                     snow</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="MD_ImagingConditionCode_terrainMasking">
               <cat:identifier>
                  <gco:ScopedName codeSpace="http://standards.iso.org/iso/19115"
                     >MD_ImagingConditionCode_terrainMasking</gco:ScopedName>
               </cat:identifier>
               <cat:definition>
                  <gco:CharacterString>the absence of collection data of a given point or area
                     caused by the relative location of topographic features which obstruct the
                     collection path between the collector(s) and the subject(s) of
                     interest</gco:CharacterString>
               </cat:definition>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
      </cat:CT_Codelist>
   </cat:codelistItem>
   <!--=== EOF ===-->
</cat:CT_CodelistCatalogue>
