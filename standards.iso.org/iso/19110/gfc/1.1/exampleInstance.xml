						<gfc:FC_RoleType codeList="http://standards.iso.org/.../Codelists.xml#FC_RoleType" codeListValue="ordinary"/>
					</gfc:type>
					<gfc:isOrdered>
						<gco:Boolean>false</gco:Boolean>
					</gfc:isOrdered>
					<gfc:isNavigable>
						<gco:Boolean>false</gco:Boolean>
					</gfc:isNavigable>
				    <gfc:rolePlayer xlink:href="#FT1"/>
