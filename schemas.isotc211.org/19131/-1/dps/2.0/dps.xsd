	<element name="TermEntry" type="dps:TermEntry_Type" substitutionGroup="gco:AbstractObject">
		<annotation>
			<documentation>datatype for definition of a term</documentation>
		</annotation>
	</element>
	<complexType name="TermEntry_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
					<element name="term" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>the term to be used for a concept</documentation>
						</annotation>
					</element>
					<element name="definition" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>representation of a concept by a descriptive statement which serves to differentiate it from related concepts</documentation>
						</annotation>
					</element>
					<element name="note" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>notes of the concept</documentation>
						</annotation>
					</element>
					<element name="termReference" type="anyURI" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>reference to an entry in a repository</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="TermEntry_PropertyType">
		<sequence minOccurs="0">
			<element ref="dps:TermEntry"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<element name="UseCase" type="dps:UseCase_Type" substitutionGroup="gco:AbstractObject">
		<annotation>
			<documentation>detailed documentation of the data product's purpose related to certain user needs</documentation>
		</annotation>
	</element>
	<complexType name="UseCase_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
					<element name="name" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>name of the use case</documentation>
						</annotation>
					</element>
					<element name="version" type="gco:CharacterString_PropertyType" minOccurs="0">
						<annotation>
							<documentation>version number of the use case</documentation>
						</annotation>
					</element>
					<element name="summary" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>comprehensive textual description of the use case</documentation>
						</annotation>
					</element>
					<element name="goal" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>short description of the goal to be achieved by a realization of the use case</documentation>
						</annotation>
					</element>
					<element name="diagram" type="anyURI" minOccurs="0">
						<annotation>
							<documentation>use case diagram represented by an URL to an image of an UML use case diagram

NOTE It is important to have a link to a diagram in form av a de-referenceable URL and not just an identifier.</documentation>
						</annotation>
					</element>
					<element name="actor" type="gco:CharacterString_PropertyType" maxOccurs="unbounded">
						<annotation>
							<documentation>designation of a user of the use case</documentation>
						</annotation>
					</element>
					<element name="stakeholder" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>company, institution or interest group concerned by the execution of the use case</documentation>
						</annotation>
					</element>
					<element name="mainSuccessScenario" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>numbered sequence of actions to be carried out during the execution of the use case</documentation>
						</annotation>
					</element>
					<element name="trigger" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>event that leads to the execution of the use case</documentation>
						</annotation>
					</element>
					<element name="precondition" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>description of the status that is required to start the execution of the use case</documentation>
						</annotation>
					</element>
					<element name="postcondition" type="gco:CharacterString_PropertyType">
						<annotation>
							<documentation>description of the status after the successful execution of the use case</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="UseCase_PropertyType">
		<sequence minOccurs="0">
			<element ref="dps:UseCase"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
