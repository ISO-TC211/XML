<?xml version="1.0" encoding="UTF-8"?>

<!-- Example of a general-grid coverage with one regular axis
     as defined in the OGC Coverage Implementation Schema 1.1.
     Last updated 2016-aug-11.
-->

<GeneralGridCoverage
    xmlns="https://schemas.isotc211.org/19123/-2/cis/2.0.0/"
    xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="https://schemas.isotc211.org/19123/-2/cis/2.0.0/ cisAll.xsd"
    gml:id="CIS_002">

    <DomainSet>
        <GeneralGrid srsName="[OGC:AnsiDate]" axisLabels="date">
            <RegularAxis axisLabel="date" uomLabel="d" lowerBound="2015-12-01" upperBound="2015-12-31" resolution="1"/>
            <gridLimits srsName="[OGC:Index1D]" axisLabels ="i">
                <IndexAxis axisLabel="i" lowerBound="1" upperBound="31"/>
            </GridLimits>
        </GeneralGrid>
    </DomainSet>

    <RangeSet>
        <DataBlock>
            <V> 1</V>
            <V> 2</V>
            <V> 3</V>
            <V> 4</V>
            <V> 5</V>
            <V> 6</V>
            <V> 7</V>
            <V> 8</V>
            <V> 9</V>
            <V>10</V>
            <V>11</V>
            <V>12</V>
            <V>13</V>
            <V>14</V>
            <V>15</V>
            <V>16</V>
            <V>17</V>
            <V>18</V>
            <V>19</V>
            <V>20</V>
            <V>21</V>
            <V>22</V>
            <V>23</V>
            <V>24</V>
            <V>25</V>
            <V>26</V>
            <V>27</V>
            <V>28</V>
            <V>29</V>
            <V>30</V>
            <v>31</v>
        </DataBlock>
    </RangeSet>

    <RangeType>
        <swe:DataRecord>
            <swe:field name="adventskalendertuerchen">
                <swe:Quantity definition="http://www.opengis.net/def/dataType/OGC/0/unsignedInt">
                    <swe:uom code="d"/>
                </swe:Quantity>
            </swe:field>
        </swe:DataRecord>
    </RangeType>

</GeneralGridCoverage>

