<?xml version="1.0" encoding="UTF-8"?>
<cts:AlignedTiling xmlns:cts="http://standards.iso.org/iso/19123/-4/cts/1.0"
                   xmlns:cis="http://www.opengis.net/cis/1.1/gml"
                   xmlns:gml="http://www.opengis.net/gml/3.2">
    
    <cts:RegularTiling identifier="id1" 
                       srsDimension="2" 
                       axisLabels="Lat Long" 
                       uomLabel="deg deg" 
                       srsName="http://www.opengis.net/def/crs/EPSG/0/4326">
        
        <cts:origin srsName="http://www.opengis.net/def/crs/EPSG/0/4326" 
                    axisLabels="Lat Long" 
                    uomLabel="deg">4 72</cts:origin>
        
        <cts:tilingBoundary srsName="http://www.opengis.net/def/crs/EPSG/8.5/4326" 
            axisLabels="Lat Long" 
            srsDimension="2">
                <cis:AxisExtent axisLabel="Lat" 
                                uomLabel="deg" 
                                lowerBound="4" 
                                upperBound="54"/>
                <cis:AxisExtent axisLabel="Long" 
                                uomLabel="deg" 
                                lowerBound="72" 
                                upperBound="140"/>
            
        </cts:tilingBoundary>
        
        <cts:TilingAxis>
            <cts:RegularTilingAxis axisLabel="Lat" 
                                   lowerBound="4" 
                                   upperBound="52" 
                                   uomLabel="deg" 
                                   span="4"/>
            <cts:padding uomLabel="deg">52 54</cts:padding>
        </cts:TilingAxis>
        
        <cts:TilingAxis>
            <cts:RegularTilingAxis axisLabel="Long" 
                                   lowerBound="72" 
                                   upperBound="138" 
                                   uomLabel="deg" 
                                   span="6"/>
            <cts:padding uomLabel="deg">138 140</cts:padding>
        </cts:TilingAxis>
        
    </cts:RegularTiling>
</cts:AlignedTiling>