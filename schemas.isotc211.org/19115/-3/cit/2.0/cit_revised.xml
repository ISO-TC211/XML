<?xml version="1.0" encoding="UTF-8"?>
<cit:party xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cit="http://schemas.isotc211.org/19115/-3/cit/2.0" 
    xmlns:mcc="http://schemas.isotc211.org/19115/-3/mcc/1.0" xmlns:gco="http://schemas.isotc211.org/19115/-3/gco/1.0"
    xsi:schemaLocation="http://standards.iso.org/iso/19115/-3/mdt/1.0 ../../mdt/2.0/mdt.xsd 
    http://schemas.isotc211.org/19115/-3/cit/2.0 cit.xsd">
    <cit:CI_Organisation>
        <cit:name/>
        <cit:contactInfo/>
        <!-- partyIdentifier: An identifier for people or organizations. -->
        <cit:partyIdentifier>
            <mcc:MD_Identifier>
                <mcc:code>
                    <gco:CharacterString>mcc:code/gco:CharacterString</gco:CharacterString>
                </mcc:code>
            </mcc:MD_Identifier>
        </cit:partyIdentifier>
        <cit:individual/>
    </cit:CI_Organisation>
</cit:party>

