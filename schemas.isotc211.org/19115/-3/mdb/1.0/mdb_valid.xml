<?xml version="1.0" encoding="UTF-8"?>
<mdb:MD_Metadata xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://standards.iso.org/iso/19115/-3/mdb/1.0 mdb.xsd" 
 xmlns:mdb="http://standards.iso.org/iso/19115/-3/mdb/1.0" 
 xmlns:gco="http://standards.iso.org/iso/19115/-3/gco/1.0"
 xmlns:mcc="http://standards.iso.org/iso/19115/-3/mcc/1.0" 
 xmlns:lan="http://standards.iso.org/iso/19115/-3/lan/1.0" 
 xmlns:cit="http://standards.iso.org/iso/19115/-3/cit/1.0">
 
   <mdb:defaultLocale>
      <lan:PT_Locale>
         <lan:language></lan:language>
         <lan:characterEncoding>
            <lan:MD_CharacterSetCode codeList="" codeListValue="UTF-16"/>
         </lan:characterEncoding>
      </lan:PT_Locale>
   </mdb:defaultLocale>
   
   <mdb:metadataScope>
      <mdb:MD_MetadataScope>
         <mdb:resourceScope>
            <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" 
               codeListValue="service">service</mcc:MD_ScopeCode>
         </mdb:resourceScope>
         <mdb:name>
            <!-- Valid for rule.mdb.scope-name -->
            <gco:CharacterString>Internal service</gco:CharacterString>
         </mdb:name>
      </mdb:MD_MetadataScope>
   </mdb:metadataScope>
   <mdb:metadataScope>
      <mdb:MD_MetadataScope>
         <mdb:resourceScope>
            <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" 
               codeListValue="series">series</mcc:MD_ScopeCode>
         </mdb:resourceScope>
         <!-- Valid for rule.mdb.scope-name due to nilReason -->
         <mdb:name gco:nilReason="unknown">
            <gco:CharacterString></gco:CharacterString>
         </mdb:name>
      </mdb:MD_MetadataScope>
   </mdb:metadataScope>
   <mdb:metadataScope>
      <mdb:MD_MetadataScope>
         <mdb:resourceScope>
            <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" 
               codeListValue="dataset">dataset</mcc:MD_ScopeCode>
         </mdb:resourceScope>
      </mdb:MD_MetadataScope>
   </mdb:metadataScope>
   
   <!-- mdb:contact required for schema validation -->
   <mdb:contact gco:nilReason="inapplicable"/>
   
   <!-- Valid for rule.mdb.create-date -->
   <mdb:dateInfo>
      <cit:CI_Date>
         <cit:date>
            <gco:DateTime>2011-11-11T00:00:00</gco:DateTime>
         </cit:date>
         <cit:dateType>
            <cit:CI_DateTypeCode codeList="codeListLocation#CI_DateTypeCode" 
               codeListValue="creation">creation</cit:CI_DateTypeCode>
         </cit:dateType>
      </cit:CI_Date>
   </mdb:dateInfo>
   <!-- Multiple creation date may be reported ?-->
   <mdb:dateInfo>
      <cit:CI_Date>
         <cit:date>
            <gco:DateTime>2012-11-11T00:00:00</gco:DateTime>
         </cit:date>
         <cit:dateType>
            <cit:CI_DateTypeCode codeList="codeListLocation#CI_DateTypeCode" 
               codeListValue="creation">creation</cit:CI_DateTypeCode>
         </cit:dateType>
      </cit:CI_Date>
   </mdb:dateInfo>
   
   <!-- mdb:identificationInfo required for schema validation -->
   <mdb:identificationInfo gco:nilReason="inapplicable"/>

</mdb:MD_Metadata>
