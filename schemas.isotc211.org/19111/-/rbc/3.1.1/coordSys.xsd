	<xs:element name="CoordinateSystem" substitutionGroup="rbc:IdentifiedObject" type="rbc:CoordinateSystem_Type"/>
	<!-- .......................................... -->
	<xs:complexType name="CoordinateSystem_Type" abstract="true">
		<xs:annotation>
			<xs:documentation>non-repeating sequence of coordinate system axes that spans a given coordinate space
				Note: A coordinate system is derived from a set of mathematical rules for specifying how coordinates in a given space are to be assigned to points. The coordinate values in a coordinate tuple shall be recorded in the order in which the coordinate system axes associations are recorded.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rbc:IdentifiedObject_Type">
				<xs:sequence>
					<!-- <xs:element name="axis" type="rbc:CoordinateSystemAxis_PropertyType" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation><xs:documentation>coordinate system axis that is a component of this coordinate system</xs:documentation></xs:annotation>
					</xs:element> -->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
