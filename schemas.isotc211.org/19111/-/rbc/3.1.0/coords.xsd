	<!-- .......................................... -->
	<xs:complexType name="DateTimeTemporalCS_Type">
		<xs:annotation>
			<xs:documentation>one-dimensional coordinate system used to record time in dateTime representation as defined in ISO 8601.
Note: A DateTimeTemporalCS shall have one axis association. It does not use axisUnitID; the temporal quantities are defined through the ISO 8601 representation.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rbc:TemporalCS_Type">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
