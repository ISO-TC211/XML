	<xs:complexType name="DerivedProjectedCS_PropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="rbc:DerivedProjectedCS"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- .......................................... -->
	<xs:element name="DerivedProjectedCS" type="rbc:DerivedProjectedCS_Type"/>
	<!-- .......................................... -->
	<xs:complexType name="DerivedProjectedCS_Type">
		<xs:annotation>
			<xs:documentation>coordinate system used by a DerivedProjected CRS, one of an affine coordinate system, a Cartesian coordinate system, a cylindrical coordinate system, an ordinal coordinate system, a polar coordinate system or a spherical coordinate system</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="affineCS" type="rbc:AffineCS_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>two- or three-dimensional coordinate system in Euclidean space with straight axes that are not necessarily orthogonal. The number of associations shall equal the dimension of the CS.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cartesianCS " type="rbc:CartesianCS_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>two- or three-dimensional coordinate system in Euclidean space with orthogonal straight axes. All axes shall have the same length unit. A CartesianCS shall have two or three axis associations; the number of associations shall equal the dimension of the CS.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cylindricalCS" type="rbc:CylindricalCS_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>three-dimensional coordinate system in Euclidean space consisting of a polar coordinate system extended by a straight coordinate axis perpendicular to the plane spanned by the polar coordinate system. A CylindricalCS shall have three axis associations.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ordinalCS" type="rbc:OrdinalCS_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>n-dimensional coordinate system in which every axis uses integers. The number of associations shall be equal the dimension of the CS.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="polarCS" type="rbc:PolarCS_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>two-dimensional coordinate system in Euclidean space in which position is specified by the distance from the origin and the angle between the line from the origin to a point and a reference direction. A PolarCS shall have two axis associations.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sphericalCS" type="rbc:SphericalCS_PropertyType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>three-dimensional coordinate system in Euclidean space with one distance measured from the origin and two angular coordinates. Not to be confused with an ellipsoidal coordinate system based on an ellipsoid "degenerated" into a sphere. A SphericalCS shall have three axis associations.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ========================================== -->
	<xs:complexType name="EllipsoidalCS_PropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="rbc:EllipsoidalCS"/>
