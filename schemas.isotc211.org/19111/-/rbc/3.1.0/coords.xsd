	<xs:complexType name="CoordinateDataType_PropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="rbc:CoordinateDataType"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- .......................................... -->
	<xs:element name="CoordinateDataType" substitutionGroup="gco:CharacterString" type="gco:CodeListValue_Type"/>
	<!-- .......................................... -->
	<xs:complexType name="CoordinateDataType_Type">
		<xs:annotation>
			<xs:documentation>datatype of coordinate values</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>compound quantity representable as a character string conformant with ISO 8601 used for a temporal coordinate system axis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="integer" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>quantity expressed as a count used for a temporal or ordinal coordinate system axis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="real" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>quantity expressed as a measure used for a temporal coordinate system axis</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
