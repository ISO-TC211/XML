	<!-- ========================================== -->
	<xs:complexType name="AxisDirection_PropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="rbc:AxisDirection"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- .......................................... -->
	<xs:element name="AxisDirection" substitutionGroup="gco:CharacterString" type="gco:CodeListValue_Type"/>
	<!-- .......................................... -->
	<xs:complexType name="AxisDirection_Type">
		<xs:annotation>
			<xs:documentation>direction of positive increase in the coordinate value for a coordinate system axis</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="aft" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Axis positive direction is aft
Note: for an observer at the centre of the object this will be towards its back, stern or tail.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="awayFrom" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is away from the object</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="clockwise" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is clockwise from a specified direction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="columnNegative" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards lower pixel column</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="columnPositive" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards higher pixel column</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="counterClockwise" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is counter clockwise from a specified direction</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="displayDown" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards bottom of approximately vertical display surface</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="displayLeft" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is left in display</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="displayRight" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is right in display</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="displayUp" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards top of approximately vertical display surface</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="down" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is down relative to gravity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="east" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is p/2 radians clockwise from north</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eastNorthEast" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately east-north-east</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eastSouthEast" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately east-south-east</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="forward" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is forward
Note: For an observer at the centre of the object this is will be towards its front, bow or nose.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="future" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>temporal axis positive direction is towards the future</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geocentricX" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is in the equatorial plane from the centre of the modelled Earth towards the intersection of the equator with the prime meridian</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geocentricY" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is in the equatorial plane from the centre of the modelled Earth towards the intersection of the equator and the meridian p/2 radians eastwards from the prime meridian</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geocentricZ" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is from the centre of the modelled Earth parallel to its rotation axis and towards its north pole</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="north" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is north
Note:  In a geodetic, geographic or projected CRS, north is defined through the geodetic reference frame. In an engineering CRS, north may be defined with respect to an engineering object rather than a geographical direction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="northEast" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately north-east</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="northNorthEast" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately north-north-east</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="northNorthWest" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately north-north-west</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="northWest" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately north-west</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="past" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>temporal axis positive direction is towards the past</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="port" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Axis positive direction is port 
Note: For an observer at the centre of the object this will be towards its left.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rowNegative" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards lower pixel row</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rowPositive" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards higher pixel row</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="south" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is p radians clockwise from north</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="southEast" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately south-east</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="southSouthEast" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately south-south-east</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="southSouthWest" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately south-south-west</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="southWest" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately south-west</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="starboard" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Axis positive direction is starboard 
Note: For an observer at the centre of the object this will be towards its right.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="towards" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is towards the object</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unspecified" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is unspecified</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="up" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is up relative to gravity</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="west" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is 3p/2 radians clockwise from north</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="westNorthWest" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately west-north-west</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="westSouthWest" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>axis positive direction is approximately west-south-west</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
