			<xs:element ref="rbc:CylindricalCS"/>
		</xs:sequence>
		<xs:attributeGroup ref="gco:ObjectReference"/>
		<xs:attribute ref="gco:nilReason"/>
	</xs:complexType>
	<!-- .......................................... -->
	<xs:element name="CylindricalCS" substitutionGroup="rbc:CoordinateSystem" type="rbc:CylindricalCS_Type"/>
	<!-- .......................................... -->
	<xs:complexType name="CylindricalCS_Type">
		<xs:annotation>
			<xs:documentation>three-dimensional coordinate system in Euclidean space consisting of a polar coordinate system extended by a straight coordinate axis perpendicular to the plane spanned by the polar coordinate system
Note: A CylindricalCS shall have three axis associations.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="rbc:CoordinateSystem_Type">
				<xs:sequence/>
			</xs:extension>
