  </annotation>
  <!-- ================================== Imports ================================== -->
  <include schemaLocation="./acquisitionInformationImagery.xsd"/>
  <import namespace="https://schemas.isotc211.org/19103/-/gco/1.1"
    schemaLocation="../../../../19103/-/gco/1.1.0/gco.xsd"/>
  <!-- <import namespace="https://schemas.isotc211.org/19136/-/gmw/1.1"
    schemaLocation="../../../../19136/-/gmw/1.1.0/gmw.xsd"/> -->
  <import namespace="https://schemas.isotc211.org/19115/-1/lan/1.3"
    schemaLocation="../../../../19115/-1/lan/1.3.0/lan.xsd"/>
  <import namespace="https://schemas.isotc211.org/19115/-1/mcc/1.3"
    schemaLocation="../../../../19115/-1/mcc/1.3.0/mcc.xsd"/>
  <!-- ########################################################################### -->
  <!-- ================================== Classes ================================= -->
  <!-- =========================================================================== -->
  <complexType name="MI_InstrumentationEventList_Type">
    <annotation>
      <documentation>Instrumentation EventList Description</documentation>
    </annotation>
    <complexContent>
      <extension base="gco:AbstractObject_Type">
        <sequence>
          <element name="citation" type="mcc:Abstract_Citation_PropertyType"/>
          <element name="description" type="gco:CharacterString_PropertyType"/>
          <element maxOccurs="1" minOccurs="0" name="locale" type="lan:PT_Locale_PropertyType"/>
          <element maxOccurs="unbounded" minOccurs="0" name="metadataConstraints"
            type="mcc:Abstract_Constraints_PropertyType"/>
          <element name="instrumentationEvent" type="mac:MI_InstrumentationEvent_PropertyType"
            minOccurs="1" maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <!-- ........................................................................ -->
  <element name="MI_InstrumentationEventList" type="mac:MI_InstrumentationEventList_Type"/>
  <!-- ........................................................................ -->
  <complexType name="MI_InstrumentationEventList_PropertyType">
    <sequence>
      <element ref="mac:MI_InstrumentationEventList" minOccurs="0"/>
    </sequence>
    <attributeGroup ref="gco:ObjectReference"/>
    <attribute ref="gco:nilReason"/>
  </complexType>
  <!-- =========================================================================== -->
  <complexType name="MI_InstrumentationEvent_Type">
    <annotation>
      <documentation>Instrumentation Event Description</documentation>
    </annotation>
    <complexContent>
      <extension base="gco:AbstractObject_Type">
        <sequence>
          <element name="citation" type="mcc:Abstract_Citation_PropertyType" minOccurs="0"
            maxOccurs="unbounded"/>
          <element name="description" type="gco:CharacterString_PropertyType"/>
          <element name="extent" type="mcc:Abstract_Extent_PropertyType" minOccurs="0"
            maxOccurs="unbounded"/>
          <element name="type" type="mac:MI_InstrumentationEventTypeCode_PropertyType" minOccurs="1"
            maxOccurs="unbounded"/>
          <element name="revisionHistory" type="mac:MI_Revision_PropertyType" minOccurs="0"
            maxOccurs="unbounded"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <!-- ........................................................................ -->
  <element name="MI_InstrumentationEvent" type="mac:MI_InstrumentationEvent_Type"/>
  <!-- ........................................................................ -->
  <complexType name="MI_InstrumentationEvent_PropertyType">
    <sequence>
      <element ref="mac:MI_InstrumentationEvent" minOccurs="0"/>
    </sequence>
    <attributeGroup ref="gco:ObjectReference"/>
    <attribute ref="gco:nilReason"/>
  </complexType>
  <!-- =========================================================================== -->
  <!-- <complexType name="MI_Revision_Type">
    <annotation>
      <documentation>Description: NASA Revision Description</documentation>
