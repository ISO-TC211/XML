        <xs:complexContent>
            <xs:extension base="lbs:NT_AdvisoryElement_Type">
                <xs:sequence>
                    <xs:element name="exit1" type="gco:Integer_PropertyType" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="exit2" type="gco:Integer_PropertyType" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ==========================================  -->
    <xs:complexType name="NT_LaneConstraint_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="lbs:NT_LaneConstraint"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- .......................................... -->
    <xs:element name="NT_LaneConstraint" type="lbs:NT_LaneConstraint_Type" substitutionGroup="lbs:NT_Constraint"/>
    <!-- .......................................... -->
    <xs:complexType name="NT_LaneConstraint_Type">
        <xs:annotation>
            <xs:documentation>This class is used to list lane restrictions. Lane numbers are counted from the normal outside edge of allowable travel (rightmost in the US
                the Americas
                and most of Europe; leftmost in the UK
                Australia
                and Japan).
                
                Allowed Lanes are ones from which the turn can be made both legally or physically if the vehicle has a large turning radius). The turning radii
                if given
                are ordered in the same manner as the allowed lanes
                and are the minimum turning radius required of a vehicle making this turn from this lane.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="lbs:NT_Constraint_Type">
                <xs:sequence>
                    <xs:element name="allowedLanes" type="gco:Integer_PropertyType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The allowed lanes from which the turn can be made. "+1" is the left most lane. Positive numbers count from the left. 
                                "-1" is the right most lane. Negative numbers count from the right.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="applicableVehicleTypes" type="lbs:NT_Vehicle_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="disallowedLanes" type="gco:Integer_PropertyType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The disallowed lanes from which the turn cannot be made. "+1" is the left most lane. Positive numbers count from the left. 
                                "-1" is the right most lane. Negative numbers count from the right.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="turningRadius" type="gco:Distance_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ==========================================  -->
    <xs:complexType name="NT_SpatialRelation_PropertyType">
