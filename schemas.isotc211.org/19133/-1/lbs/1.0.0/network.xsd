    <!-- .......................................... -->
    <xs:element name="NT_SpatialRelation" substitutionGroup="gco:CharacterString" type="gco:CodeListValue_Type"/>
    <!--    <xs:element name="NT_SpatialRelation" type="lbs:NT_SpatialRelation_Type"/> -->
    <!-- .......................................... -->
    <xs:complexType name="NT_SpatialRelation_Type">
        <xs:sequence>
            <xs:element name="leftOfRoute" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="overRoute" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="rightOfRoute" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
