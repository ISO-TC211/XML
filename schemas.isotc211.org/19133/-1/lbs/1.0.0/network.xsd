            <xs:element name="allVehicles" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="bicycle" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="carWithTrailer" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="dangerousCargo" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="deliveryTruck" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="disableOccupant" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="emergency" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="employee" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="explosiveCargo" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="facility" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="farm" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="hov" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="lightRail" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="mailDelivery" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="military" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="moped" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="motorcycle" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="passengerCars" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="pedestrian" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="pollutingToWaterCargo" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="privateBus" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="publicBus" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="residential" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="schoolBus" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="snowChains" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="tanker" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="taxi" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="transportTruck" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="trolleyBus" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="truck" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
