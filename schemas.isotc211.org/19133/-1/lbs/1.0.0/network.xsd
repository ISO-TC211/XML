            <xs:element name="exitPlace" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
            <xs:element name="highwayMerge" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1"/>
