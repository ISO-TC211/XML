    <!-- ==========================================  -->
    <xs:complexType name="NT_TemporalConstraint_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="lbs:NT_TemporalConstraint"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- .......................................... -->
    <xs:element name="NT_TemporalConstraint" type="lbs:NT_TemporalConstraint_Type"  substitutionGroup="lbs:NT_Constraint"/>
    <!-- .......................................... -->
    <xs:complexType name="NT_TemporalConstraint_Type">
        <xs:annotation>
            <xs:documentation>This class specifies when a network entity may be traversed. The constraint may be listed in two distinct manner
                when the network entity is close to traffic
                or when it is opened. Co-constraints on this entity may further specify vehicles types.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="lbs:NT_Constraint_Type">
                <xs:sequence>
                    <xs:element name="allowedTime" type="gml:TimePrimitivePropertyType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Time when the entity is open for use. If NULL, the value is assumed to be the complement of the "forbiddenTime."</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="forbiddenTime" type="gml:TimePrimitivePropertyType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Time when the entity is closed for use. If NULL, the value is assumed to be the complement of the "allowedTime."</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ==========================================  -->
    <xs:complexType name="NT_Vehicle_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="lbs:NT_Vehicle"/>
        </xs:sequence>
