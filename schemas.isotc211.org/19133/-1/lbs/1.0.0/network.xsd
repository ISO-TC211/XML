    <!-- ==========================================  -->
    <xs:complexType name="NT_Constraint_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="lbs:NT_Constraint"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- .......................................... -->
    <xs:element name="NT_Constraint" type="lbs:NT_Constraint_Type" abstract="true"/>
    <!-- .......................................... -->
    <xs:complexType name="NT_Constraint_Type">
        <xs:annotation>
            <xs:documentation>This class is  an abstract class whose subtypes specify turn and link restrictions.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="description" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>This is a natural language description of the constraint, and may contain additional advisory information to be used in creatng instructions that this constraint plays a role in. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="temporalValidity" type="gml:TimePrimitivePropertyType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>The attribute "temporalValidity" specifies the times at which this constraint is valid. In a temproal constraint, the temporalValidity may be used to indicate a larter time frame within which the temporal constaint is valid. For example, the temporalValidity may be "April-September" while the temproal constraint  list "Monday-Friday, 8AM to 10AM" meaing that the constaint is active only during those months, those days and between those times. </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coConstraint" type="lbs:NT_Constraint_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <!-- ==========================================  -->
    <xs:complexType name="NT_ExitAssociation_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="lbs:NT_ExitAssociation"/>
