				<xs:sequence>
					<xs:element name="measurementsBeforeLaunch" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating whether measurement before launch have been conducted</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="measurementsAfterLaunch" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating whether measurement after launch have been conducted</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="participationDiffGroups" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the participation of more than one group</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="archivingOfData" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating a guaranteed long-term archiving of data</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="scaleDiffAcknowledged" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating an acknowledgement of the influence of scale differences</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="temporalErrorsQuantified" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating an assessment of temporal errors</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="spatialErrorsQuantified" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating an assessment of spatial errors</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="sustainedFieldMeasOverLifeSensor" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating a commitment to sustained field measurements over the life of the sensor</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="simulationEstimatedAccuracy" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the existence of a simulation of the estimated accuracy</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="assessedAccuracyAfterLaunch" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the existence of a simulation of the estimated accuracy after launch</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="snowCover" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>percentage of snow-cover</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="iceCover" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>percentage of ice-cover</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="leafAreaIndex" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>dimensionless quantity that characterizes plant canopies</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="aerosolOpticalThickness" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>aerosol load, expressed in optical depth at a given wavelength (865 nm)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="waterVapour" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>relative humidity: how much water the air currently contains as compared to what it would contain if saturated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="continuousOrDiscreteData" type="gco:Integer_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the generation of long, continuous data</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="surfaceReflectanceVegetationIndex" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the option of determination of the vegetation index</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="primaryProductivity" type="gco:Real_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>rate at which energy is converted to organic substances by photosynthetic producers</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="fire" type="gco:Integer_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>number of fires</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="landCoverLandChange" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the existence of the change of land cover</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="higherResolutionSensor" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>flag indicating the application of a higher resolution satellite sensor</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
