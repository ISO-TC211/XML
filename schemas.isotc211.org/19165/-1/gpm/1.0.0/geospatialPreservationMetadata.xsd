	<import namespace="https://schemas.isotc211.org/19115/-1/mda/1.3" schemaLocation="../../../../19115/-1/mda/1.3.1/mda.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mdb/1.3" schemaLocation="../../../../19115/-1/mdb/1.3.1/mdb.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mmi/1.3" schemaLocation="../../../../19115/-1/mmi/1.3.0/mmi.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mpc/1.3" schemaLocation="../../../../19115/-1/mpc/1.3.0/mpc.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mrc/1.3" schemaLocation="../../../../19115/-1/mrc/1.3.0/mrc.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mrd/1.3" schemaLocation="../../../../19115/-1/mrd/1.3.0/mrd.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mri/1.3" schemaLocation="../../../../19115/-1/mri/1.3.0/mri.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mrl/1.3" schemaLocation="../../../../19115/-1/mrl/1.3.0/mrl.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/mrs/1.3" schemaLocation="../../../../19115/-1/mrs/1.3.1/mrs.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/msr/1.3" schemaLocation="../../../../19115/-1/msr/1.3.1/msr.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-1/srv/1.3" schemaLocation="../../../../19115/-1/srv/1.3.0/srv.xsd"/>
	<import namespace="https://schemas.isotc211.org/19115/-2/mac/2.2" schemaLocation="../../../../19115/-2/mac/2.2.0/mac.xsd"/>
	<import namespace="https://schemas.isotc211.org/19157/-1/dqc/1.0" schemaLocation="../../../../19157/-1/dqc/1.0.1/dqc.xsd"/>
	<import namespace="https://schemas.isotc211.org/19157/-1/mdq/1.0" schemaLocation="../../../../19157/-1/mdq/1.0.1/mdq.xsd"/>
	<import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
	<!--XML Schema document initially created by ShapeChange - http://shapechange.net/
	                        modified by hand (EJB)-->
	<!-- This schema represents the combined information available from: 
		ISO 19165-1:2018 data dictionary; and 
		HMMG model ISO 19165 Preservation of digital data and metadata:ISO 19165-1 Edition 1 
	    The modifications allow it to reference ISO 19157-1:2023 -->
	<!-- =========================================================================== -->
	<element name="GP_PreservationMetadata" type="gpm:GP_PreservationMetadata_Type" substitutionGroup="mdb:MD_Metadata">
		<annotation><documentation>root entity which defines preservation metadata about a resource or resources</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_PreservationMetadata_Type">
		<complexContent>
			<extension base="mdb:MD_Metadata_Type">
				<sequence>
					<element name="acquisition" type="gpm:GP_Acquisition_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>details about the archival acquisition</documentation></annotation>
					</element>
					<element name="associatedResource" type="gpm:GP_AssociatedResource_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>associated resource information</documentation></annotation>
					</element>
					<element name="archivalInfo" type="gpm:GP_PackagingInformation_PropertyType" minOccurs="1" maxOccurs="unbounded">
						<annotation><documentation>description of the information package</documentation></annotation>
					</element>
					<element name="resourceUsage" type="gpm:GP_Usage_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>basic information about specific application(s) for which the resource has been or is being used by different use</documentation></annotation>
					</element>
					<element name="valueAndStrategy" type="gpm:GP_ValueAndPreservationStrategy_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>requirements for assessing the value of the resource and its future preservation strateg</documentation></annotation>
					</element>
					<element name="preservationActions" type="mrl:LI_Lineage_PropertyType" minOccurs="0">
						<annotation><documentation>information about the provenance, source(s), and/or the production process(es) applied to the resource by the archive.</documentation></annotation>
					</element>
					
					<!-- ? <element name="informationValue" type="gpm:GP_InformationValue_PropertyType" minOccurs="0" maxOccurs="unbounded"/> -->
					<!-- name change <element name="packagingInformation" type="gpm:GP_PackagingInformation_PropertyType" maxOccurs="unbounded"/> -->
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_PreservationMetadata_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_PreservationMetadata"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_Usage" type="gpm:GP_Usage_Type" substitutionGroup="mri:MD_Usage">
		<annotation><documentation>brief description of ways in which the resource(s) is/are currently or has been used</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_Usage_Type">
		<complexContent>
			<extension base="mri:MD_Usage_Type">
				<sequence>
					<element name="service" type="gpm:GP_ServiceIdentification_PropertyType">
						<annotation><documentation>services that currently or has been using this resource(s)</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_Usage_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_Usage"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_ValueAndPreservationStrategy" type="gpm:GP_ValueAndPreservationStrategy_Type">
		<annotation>
			<documentation>reason, justification and strategy for archiving and curating the resource(s)</documentation>
		</annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_ValueAndPreservationStrategy_Type">
		<sequence>
			<element name="preservationStrategyPolicy" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1">
				<annotation><documentation>document reference with the archive preservation strategy</documentation></annotation>
			</element>
			<element name="nextReview" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<annotation><documentation>information about the next foreseen review of the archived package including date and intention/reason</documentation></annotation>
			</element>
			<element name="informationValue" type="gpm:GP_InformationValue_PropertyType" minOccurs="0" maxOccurs="unbounded">
				<annotation><documentation>reason and justification of the value of the resource</documentation></annotation>
			</element>
			<element name="remove" type="gpm:GP_Remove_PropertyType" minOccurs="0" maxOccurs="1">
				<annotation><documentation>foreseen date for the finalization of the preservation</documentation></annotation>
			</element>
			<element name="review" type="gpm:GP_Review_PropertyType" minOccurs="0" maxOccurs="unbounded">
				<annotation><documentation>periodicity of the foreseen reviews</documentation></annotation>
			</element>
			<element name="reviewPeriod" type="gpm:GP_ReviewPeriod_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_ValueAndPreservationStrategy_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_ValueAndPreservationStrategy"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_Acquisition" type="gpm:GP_Acquisition_Type" substitutionGroup="gco:AbstractObject">
		<annotation><documentation>information about the acquisition of the resource(s)</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_Acquisition_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
					<element name="donor" type="cit:CI_Responsibility_PropertyType" minOccurs="1" maxOccurs="unbounded">
						<annotation><documentation>party responsible that delivered the resource(s) to the archive</documentation></annotation>
					</element>
					<element name="date" type="cit:CI_Date_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>date of the delivery of the resource(s) to the archive</documentation></annotation>
					</element>
					<element name="acquisitionEvent" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>description of the acquisition process</documentation></annotation>
					</element>
					<element name="acquisitionStatus" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>status of the acquisition process</documentation></annotation>
					</element>
					<element name="rightsClearance" type="mco:MD_Constraints_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>statement describing how and with whom the resource can be shared</documentation></annotation>
					</element>
					<element name="preservationCopiesAllowed" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>indication of whether or not the resource(s) has can be duplicated by the archive</documentation></annotation>
					</element>
					<element name="redistributionAllowed" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>indication of whether or not the resource(s) can be redistributed</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_Acquisition_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_Acquisition"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_AssociatedResource" type="gpm:GP_AssociatedResource_Type" substitutionGroup="mri:MD_AssociatedResource">
		<annotation><documentation>definition</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_AssociatedResource_Type">
		<complexContent>
			<extension base="mri:MD_AssociatedResource_Type">
				<sequence>
					<element name="relationType" type="gpm:GP_RelationTypeCode_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>relation dependency</documentation></annotation>
					</element>
					<element name="associationProcess" type="mrl:LI_ProcessStep_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>process that made the relation between this resource and the cited resource</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_AssociatedResource_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_AssociatedResource"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_Identification" type="gpm:GP_Identification_Type" substitutionGroup="mri:AbstractMD_Identification">
		<annotation><documentation>identification of the archived resource(s)</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_Identification_Type">
		<complexContent>
			<extension base="mri:AbstractMD_Identification_Type">
				<sequence>
					<element name="citationStatement" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>indication on how you have to cite or knowledge the attribution of the resource(s)</documentation></annotation>
					</element>
					<element name="reasonForCreation" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>reason for creating the resource. In many cases this is different from the “purpose”.
							In the citations title one can use “legal mandate” or “data continuity of the temporal series”. If there is a legal mandate, one can be more precise by fully citing a legal document or a contract containing the reason or the mandate.</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_Identification_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_Identification"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_Format" type="gpm:GP_Format_Type" substitutionGroup="mcc:Abstract_Format">
		<annotation><documentation>format stored in the archive for this resource(s).</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_Format_Type">
		<complexContent>
			<extension base="mcc:Abstract_Format_Type">
				<sequence>
					<element name="formatSpecificationProfile" type="cit:CI_Citation_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>cites a document explaining how the file format has been used to match the needs of the product. Sometimes this is called a “format specification profile” ('formatSpecificationCitation' links to the file format specification document (e.g the document describing the TIFF 6.0 format).</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_Format_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_Format"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_ReviewPeriod" type="gpm:GP_ReviewPeriod_Type" substitutionGroup="gco:AbstractObject">
		<annotation><documentation>information about the review policy and period of the archiving resource</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_ReviewPeriod_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
			<element name="responsibility" type="cit:CI_Responsibility_PropertyType" minOccurs="0" maxOccurs="1">
				<annotation><documentation>responsible party for the revision of this resource of the archive</documentation></annotation>
			</element>
			<element name="date" type="cit:CI_Date_PropertyType" minOccurs="1" maxOccurs="1">
				<annotation><documentation>date of the next foreseen review</documentation></annotation>
			</element>
			<element name="retentionDocument" type="cit:CI_Citation_PropertyType" minOccurs="1" maxOccurs="1">
				<annotation><documentation>reference to a document that describes the revision procedure</documentation></annotation>
			</element>
			<element name="gathering" type="mmi:MD_MaintenanceInformation_PropertyType" minOccurs="0" maxOccurs="unbounded">
				<annotation><documentation>information about the periodicity of the revision</documentation></annotation>
			</element>
		</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_ReviewPeriod_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_ReviewPeriod"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_InformationValue" type="gpm:GP_InformationValue_Type" substitutionGroup="gco:AbstractObject">
		<annotation><documentation>reason and justification for archiving the resource</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_InformationValue_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
					<element name="type" type="gpm:GP_InformationValueTypeCode_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>the reason for archiving the resource and gives it value</documentation></annotation>
					</element>
					<element name="comments" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>additional explanation about the reason for archiving the resource</documentation></annotation>
					</element>
					<element name="dueDate" type="cit:CI_Date_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>expiration date of the reason for archiving the resource</documentation></annotation>
					</element>
					<element name="justification" type="gpm:GP_JustificationValue_PropertyType" minOccurs="0" maxOccurs="unbounded">
						<annotation><documentation>justification of importance of the resource in context of the general reason</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_InformationValue_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_InformationValue"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_Remove" type="gpm:GP_Remove_Type" substitutionGroup="gco:AbstractObject">
		<annotation><documentation>information about the intent of removing the resource in the future or evidence that the resource has been removed.</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_Remove_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
					<element name="dueDate" type="cit:CI_Date_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>finalization date for the archival period. After this period, the resource will not be longer preserved</documentation></annotation>
					</element>
					<element name="removed" type="gco:Boolean_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>indication of whether or not the resource(s) has been removed and is no longer archived</documentation></annotation>
					</element>
					<element name="removalAuthority" type="cit:CI_Responsibility_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>responsible party that has the decision on removing the resource form the archive</documentation></annotation>
					</element>
		</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_Remove_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_Remove"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_Review" type="gpm:GP_Review_Type">
		<annotation><documentation>information about a review (foreseen or done)</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_Review_Type">
		<sequence>
			<element name="date" type="cit:CI_Date_PropertyType" minOccurs="1" maxOccurs="1">
				<annotation><documentation>date of the review</documentation></annotation>
			</element>
			<element name="reason" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<annotation><documentation>reason for the review</documentation></annotation>
			</element>
			<element name="outcome" type="gco:CharacterString_PropertyType" minOccurs="0" maxOccurs="1">
				<annotation><documentation>outcome of the review</documentation></annotation>
			</element>
			<element name="resposibility" type="cit:CI_Responsibility_PropertyType" minOccurs="0" maxOccurs="1">
				<annotation><documentation>responsible party for the review</documentation></annotation>
			</element>
		</sequence>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_Review_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_Review"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
		<!-- =========================================================================== -->
	<element name="GP_JustificationValue" type="gpm:GP_JustificationValue_Type" substitutionGroup="gco:AbstractObject">
		<annotation><documentation>explicit justification why this resource is important in the general context</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_JustificationValue_Type">
		<complexContent>
			<extension base="gco:AbstractObject_Type">
				<sequence>
					<element name="justification" type="gco:CharacterString_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>statement on why this resource is important</documentation></annotation>
					</element>
					<element name="justificationType" type="gpm:GP_JustificationValueTypeCode_PropertyType" minOccurs="0">
						<annotation><documentation>justification why this resource is important</documentation></annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ........................................................................ -->
	<complexType name="GP_JustificationValue_PropertyType">
		<sequence minOccurs="0">
			<element ref="gpm:GP_JustificationValue"/>
		</sequence>
		<attributeGroup ref="gco:ObjectReference"/>
		<attribute ref="gco:nilReason"/>
	</complexType>
	<!-- =========================================================================== -->
	<element name="GP_ServiceIdentification" type="gpm:GP_ServiceIdentification_Type" substitutionGroup="srv:SV_ServiceIdentification">
		<annotation><documentation>estimation of the usage of the resource by registering a service and service metrics that host or hosted the resource</documentation></annotation>
	</element>
	<!-- ........................................................................ -->
	<complexType name="GP_ServiceIdentification_Type">
		<complexContent>
			<extension base="srv:SV_ServiceIdentification_Type">
				<sequence>
					<element name="operator" type="cit:CI_Responsibility_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>Who was (or is) the responsible party maintaining the service</documentation></annotation>
					</element>
					<element name="stillActive" type="gco:Boolean_PropertyType" minOccurs="1" maxOccurs="1">
						<annotation><documentation>indication of whether or not the service has been retired and is no longer active</documentation></annotation>
					</element>
					<element name="retirementDate" type="cit:CI_Date_PropertyType" minOccurs="0" maxOccurs="1">
						<annotation><documentation>retirement date of the service (in the past or in the future).</documentation></annotation>
					</element>
