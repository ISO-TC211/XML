<?xml version="1.0" encoding="UTF-8"?>
<mrl:LI_Lineage  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:mrl="http://standards.iso.org/iso/19115/-3/mrl/2.0"
    xmlns:mcc="http://standards.iso.org/iso/19115/-3/mcc/1.0" 
    xmlns:gco="http://standards.iso.org/iso/19115/-3/gco/1.0"
    xmlns:gex="http://standards.iso.org/iso/19115/-3/gex/1.0"
    xmlns:mri="http://standards.iso.org/iso/19115/-3/mri/1.0"
    xmlns:mrs="http://standards.iso.org/iso/19115/-3/mrs/1.0"
    xmlns:cit="http://standards.iso.org/iso/19115/-3/cit/2.0"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xsi:schemaLocation="http://standards.iso.org/iso/19115/-3/mrl/2.0 ../../mds/2.0/mds.xsd
    http://standards.iso.org/iso/19115/-3/cit/2.0 ../../cit/2.0/cit.xsd">
    <mrl:statement>
        <gco:CharacterString>mrl:statement/gco:CharacterString</gco:CharacterString>
    </mrl:statement>
    <mrl:scope>
        <mcc:MD_Scope>
            <mcc:level>
                <mcc:MD_ScopeCode codeList="codeListLocation#MD_ScopeCode" codeListValue="gmd:level/gmd:MD_ScopeCode">mcc:level/mcc:MD_ScopeCode</mcc:MD_ScopeCode>
            </mcc:level>
            <mcc:extent>
                <gex:EX_Extent>
                    <gex:description>
                        <gco:CharacterString>gex:description/gco:CharacterString</gco:CharacterString>
                    </gex:description>
                </gex:EX_Extent>
            </mcc:extent>
            <mcc:levelDescription>
                <mcc:MD_ScopeDescription>
                    <mcc:other>
                        <gco:CharacterString>mcc:other/gco:CharacterString</gco:CharacterString>
                    </mcc:other>
                </mcc:MD_ScopeDescription>
            </mcc:levelDescription>
        </mcc:MD_Scope>
    </mrl:scope>
    <mrl:source>
        <mrl:LI_Source id="lineageSource">
            <mrl:description>
                <gco:CharacterString>mrl:description/gco:CharacterString</gco:CharacterString>
            </mrl:description>
            <mrl:sourceSpatialResolution>
                <mri:MD_Resolution>
                    <mri:equivalentScale>
                        <mri:MD_RepresentativeFraction>
                            <mri:denominator>
                                <gco:Integer>444</gco:Integer>
                            </mri:denominator>
                        </mri:MD_RepresentativeFraction>
                    </mri:equivalentScale>
                </mri:MD_Resolution>
            </mrl:sourceSpatialResolution>
            <mrl:sourceReferenceSystem>
                <mrs:MD_ReferenceSystem>
                    <mrs:referenceSystemIdentifier>
                        <mcc:MD_Identifier>
                            <mcc:code>
                                <gco:CharacterString>mcc:code/gco:CharacterString</gco:CharacterString>
                            </mcc:code>
                        </mcc:MD_Identifier>
                    </mrs:referenceSystemIdentifier>
                </mrs:MD_ReferenceSystem>
            </mrl:sourceReferenceSystem>
            <mrl:sourceCitation>
                <cit:CI_Citation>
                    <cit:title>
                        <gco:CharacterString>cit:title/gco:CharacterString</gco:CharacterString>
                    </cit:title>
                    <cit:date gco:nilReason="missing"/>
                </cit:CI_Citation>
            </mrl:sourceCitation>
            <mrl:scope>
                <mcc:MD_Scope>
                    <mcc:level/>
                    <mcc:extent>
                        <gex:EX_Extent>
                            <gex:description>
                                <gco:CharacterString>gex:description/gco:CharacterString</gco:CharacterString>
                            </gex:description>
                        </gex:EX_Extent>
                    </mcc:extent>
                    <mcc:levelDescription/>
                </mcc:MD_Scope>
            </mrl:scope>
            <mrl:sourceStep xlink:href="#lineageProcessStep"/>
            <mrl:sourceStep>
                <mrl:LI_ProcessStep id="embeddedLineageProcessStep">
                    <mrl:description>
                        <gco:CharacterString>mrl:description/gco:CharacterString</gco:CharacterString>
                    </mrl:description>
                    <mrl:stepDateTime>
                        <gml:TimeInstant gml:id="d10e2566">
                            <gml:timePosition>gml:TimeInstant/gml:timePosition</gml:timePosition>
                        </gml:TimeInstant>
                    </mrl:stepDateTime>
                    <mrl:processor/>
                </mrl:LI_ProcessStep>
            </mrl:sourceStep>
        </mrl:LI_Source>
    </mrl:source>
    <mrl:processStep>
        <mrl:LI_ProcessStep id="lineageProcessStep">
            <mrl:description>
                <gco:CharacterString>mrl:description/gco:CharacterString</gco:CharacterString>
            </mrl:description>
            <mrl:stepDateTime>
                <gml:TimeInstant gml:id="d10e2499">
                    <gml:timePosition>gml:TimeInstant/gml:timePosition</gml:timePosition>
                </gml:TimeInstant>
            </mrl:stepDateTime>
            <mrl:processor>
                <cit:CI_Responsibility>
                    <cit:role>
                        <cit:CI_RoleCode codeList="codeListLocation#CI_RoleCode" codeListValue="gmd:role/gmd:CI_RoleCode">cit:role/cit:CI_RoleCode</cit:CI_RoleCode>
                    </cit:role>
                    <cit:party>
                        <cit:CI_Individual>
                            <cit:name>
                                <gco:CharacterString>cit:name/gco:CharacterString</gco:CharacterString>
                            </cit:name>
                        </cit:CI_Individual>
                    </cit:party>
                </cit:CI_Responsibility>
            </mrl:processor>
            <mrl:source xlink:href="#lineageSource"/>
            <mrl:source>
                <mrl:LI_Source id="embeddedLineageSource">
                    <mrl:description>
                        <gco:CharacterString>mrl:description/gco:CharacterString</gco:CharacterString>
                    </mrl:description>
                    <mrl:sourceSpatialResolution>
                        <mri:MD_Resolution>
                            <mri:equivalentScale>
                                <mri:MD_RepresentativeFraction>
                                    <mri:denominator>
                                        <gco:Integer>123</gco:Integer>
                                    </mri:denominator>
                                </mri:MD_RepresentativeFraction>
                            </mri:equivalentScale>
                        </mri:MD_Resolution>
                    </mrl:sourceSpatialResolution>
                    <mrl:sourceCitation>
                        <cit:CI_Citation>
                            <cit:title>
                                <gco:CharacterString>
                                    >cit:title/gco:CharacterString</gco:CharacterString>
                            </cit:title>
                            <cit:date gco:nilReason="missing"/>
                        </cit:CI_Citation>
                    </mrl:sourceCitation>
                    <mrl:scope>
                        <mcc:MD_Scope>
                            <mcc:level/>
                            <mcc:extent>
                                <gex:EX_Extent>
                                    <gex:description>
                                        <gco:CharacterString>gex:description/gco:CharacterString</gco:CharacterString>
                                    </gex:description>
                                </gex:EX_Extent>
                            </mcc:extent>
                            <mcc:levelDescription/>
                        </mcc:MD_Scope>
                    </mrl:scope>
                </mrl:LI_Source>
            </mrl:source>
        </mrl:LI_ProcessStep>
    </mrl:processStep>
    <mrl:processStep>
        <mrl:LE_ProcessStep>
            <mrl:description/>
            <mrl:processingInformation>
                <mrl:LE_Processing>
                    <mrl:algorithm>
                        <mrl:LE_Algorithm>
                            <mrl:citation/>
                            <mrl:description/>
                        </mrl:LE_Algorithm>
                    </mrl:algorithm>
                    <mrl:identifier/>
                    <mrl:softwareReference/>
                    <mrl:procedureDescription/>
                    <mrl:documentation/>
                    <mrl:runTimeParameters/>
                    <mrl:parameter>
                        <mrl:LE_ProcessParameter>
                            <mrl:name>
                                <gco:MemberName>
                                    <gco:aName>
                                        <gco:CharacterString>gco:aName/gco:CharacterString</gco:CharacterString>
                                    </gco:aName>
                                    <gco:attributeType>
                                        <gco:TypeName>
                                            <gco:aName>
                                                <gco:CharacterString>gco:aName/gco:CharacterString</gco:CharacterString>
                                            </gco:aName>
                                        </gco:TypeName>
                                    </gco:attributeType>
                                </gco:MemberName>
                            </mrl:name>
                            <mrl:direction>
                                <mrl:LE_ParameterDirection>in</mrl:LE_ParameterDirection>
                            </mrl:direction>
                            <mrl:description><gco:CharacterString>mrl:description/gco:CharacterString</gco:CharacterString></mrl:description>
                            <mrl:optionality>
                                <gco:Boolean>false</gco:Boolean>
                            </mrl:optionality>
                            <mrl:repeatability><gco:Boolean>true</gco:Boolean></mrl:repeatability>
                            <mrl:valueType><gco:RecordType>mrl:valueType/gco:RecordType</gco:RecordType></mrl:valueType>
                            <mrl:value><gco:Record>mrl:value/gco:Record</gco:Record></mrl:value>
                            <mrl:resource>
                                <mrl:LE_Source/>
                            </mrl:resource>
                        </mrl:LE_ProcessParameter>
                    </mrl:parameter>
                    <mrl:parameter>
                        <mrl:LE_ProcessParameter>
                            <mrl:name/>
                            <mrl:direction/>
                            <mrl:description/>
                            <mrl:optionality/>
                            <mrl:repeatability/>
                            <mrl:valueType/>
                            <mrl:value/>
                            <mrl:resource>
                                <mrl:LI_Source/>
                            </mrl:resource>
                        </mrl:LE_ProcessParameter>
                    </mrl:parameter>
                    <mrl:otherPropertyType/>
                    <mrl:otherProperty/>
                </mrl:LE_Processing>
            </mrl:processingInformation>
        </mrl:LE_ProcessStep>
    </mrl:processStep>
</mrl:LI_Lineage>

